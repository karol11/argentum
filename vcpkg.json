{
  "name": "argentum",
  "version": "0.0.20",
  "dependencies": [
    "sdl2",
    "opengl",
    "sqlite3",
    "curl",
    "sdl2-ttf",
    "sdl2-mixer",
    {
      "name": "sdl2-image",
      "features": ["libjpeg-turbo"]
    },
    {
      "name": "llvm",
      "default-features":false,
      "features": ["target-webassembly", "target-x86", "target-aarch64"]
    },
    {
      "name": "skia",
      "default-features": false,
      "features":["gl", "graphite","freetype", "fontconfig", "harfbuzz", "icu"]
    }
  ]
}
