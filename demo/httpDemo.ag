using httpClient { get, Response }
using sys { Blob, log, String, setMainObject }
using string;

const xTestUrl = "https://aglang.org";
//const xTestUrl = "http://localhost";

class App {}
app = App;
setMainObject(app);

get(xTestUrl, app.&onFetched(resp Response){
    log(`Result code=${resp.status}: `);
    s = String;
    s.fromBlob(resp.body, 0, 40);
    log(s);
    setMainObject(?App);
})
